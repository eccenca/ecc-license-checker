{
  "name": "ecc-license-checker",
  "description": "Check license info for a package",
  "author": "eccenca GmbH <opensource@eccenca.com> (http://eccenca.com)",
  "version": "2.0.2",
  "dependencies": {
    "ansi-escape-sequences": "^2.2.2",
    "chalk": "^0.5.1",
    "clear": "0.0.1",
    "cli-table": "^0.3.1",
    "command-line-args": "^2.1.6",
    "command-line-commands": "^1.0.2",
    "inquirer": "^1.0.2",
    "js-yaml": "^3.5.2",
    "license-checker": "^5.1.2",
    "lodash": "^4.2.1",
    "mkdirp": "^0.5.1",
    "spdx": "^0.5.1"
  },
  "devDependencies": {
    "ecc-dotfiles": "^1.4.1",
    "ecc-gulp-tasks": "^2.0.2",
    "ecc-test-helpers": "^15.0.0"
  },
  "main": "./es5/index.js",
  "bin": {
    "ecc-license-checker": "./bin/license-checker"
  },
  "scripts": {
    "init": "ecc-link-dotfiles",
    "build": "gulp build",
    "test": "gulp test",
    "download-spdx": "curl 'https://spdx.org/licenses/licenses.json' --no-buffer > temp.json",
    "spdx": "npm run download-spdx && jq . temp.json > data/licenses.json && rm -f temp.json",
    "prepublish": "npm run init && npm run test && npm run build"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.eccenca.com:8101/elds-ui/ecc-license-checker.git"
  }
}
