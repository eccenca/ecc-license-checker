{
  "name": "ecc-license-checker",
  "description": "Check license info for a package",
  "author": "eccenca GmbH <opensource@eccenca.com> (http://eccenca.com)",
  "version": "2.4.1",
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "clear": "0.0.1",
    "cli-table": "^0.3.1",
    "command-line-args": "^3.0.4",
    "command-line-commands": "^1.0.2",
    "command-line-usage": "^3.0.8",
    "inquirer": "^1.0.2",
    "js-yaml": "^3.5.2",
    "license-checker": "^8.0.3",
    "lodash": "^4.2.1",
    "mkdirp": "^0.5.1",
    "spdx": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-eccenca": "^1.0.3",
    "babel-register": "^6.18.0",
    "ecc-dotfiles": "^1.9.1",
    "mocha": "^3.2.0",
    "should": "^11.1.2"
  },
  "main": "./es5/index.js",
  "bin": {
    "ecc-license-checker": "./bin/license-checker"
  },
  "scripts": {
    "init": "ecc-link-dotfiles",
    "build": "babel src -d lib",
    "test": "mocha --compilers js:babel-register",
    "download-spdx": "curl 'https://spdx.org/licenses/licenses.json' --no-buffer > temp.json",
    "spdx": "npm run download-spdx && jq . temp.json > data/licenses.json && rm -f temp.json",
    "prepublish": "npm run init && npm run test && npm run build"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/elds/ecc-license-checker.git"
  },
  "bugs": {
    "url": "https://github.com/elds/ecc-license-checker/issues"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  }
}
